<li class='clearfix record <%= cycle("on", "on-hover") %>' id="<%= dom_id(fieldtrip) -%>">
  <span class='title'>
    <%= fieldtrip.Name %>
    <span class="preview">&nbsp;</span>
  </span>
  <span class='actions'>
    <%= link_to refinery_icon_tag("application_go.png"), fieldtrip_path(fieldtrip),
        :title => t('.view_live_html'),
        :target => "_blank" %>
    <%= link_to refinery_icon_tag("application_edit.png"), edit_admin_fieldtrip_path(fieldtrip),
         :title => t('.edit') %>
    <%= link_to refinery_icon_tag("delete.png"), admin_fieldtrip_path(fieldtrip),
        :class => "cancel confirm-delete",
        :title => t('.delete'),
        :confirm => t('message', :scope => 'shared.admin.delete', :title => fieldtrip.Name),
        :method => :delete %>
  </span>
</li>
